<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper	namespace="cn.ibizlab.pms.core.ibizpro.mapper.IBZProSequenceMapper">
	
	<!-- 无视图模式 -->
	<sql id="select" databaseId="mysql">
        <![CDATA[ SELECT t1.`BEGINSEQ`, t1.`CATEGORY`, t1.`CREATEDATE`, t1.`CREATEMAN`, t1.`CURSEQ`, t1.`DEPTID`, t1.`IBZPRO_SEQUENCEID`, t1.`IBZPRO_SEQUENCENAME`, t1.`INCREMENT`, t1.`ISENABLE`, t1.`ORGID`, t1.`UPDATEDATE`, t1.`UPDATEMAN`, t1.`YEAR` FROM `T_IBZPRO_SEQUENCE` t1   ]]>
    </sql>

	
	<!-- 搜索条件 -->
	<sql id="searchMode" databaseId="mysql">
		<if test="srf.n_ibzpro_sequencename_eq != null ">
			<![CDATA[ AND  t1.`IBZPRO_SEQUENCENAME` = #{srf.n_ibzpro_sequencename_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_ibzpro_sequencename_like != null ">
            <bind name="bind_n_ibzpro_sequencename_like" value="'%' + srf.n_ibzpro_sequencename_like + '%'" />
            <![CDATA[ AND  t1.`IBZPRO_SEQUENCENAME` like #{bind_n_ibzpro_sequencename_like,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_category_eq != null ">
			<![CDATA[ AND  t1.`CATEGORY` = #{srf.n_category_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="ew!=null and ew.sqlSegment!=null and ew.sqlSegment!=''">
		AND ${ew.sqlSegment}
		</if>
	</sql>


    <!--该方法用于重写mybatis中selectById方法，以实现查询逻辑属性-->
	<select id="selectById"  resultMap="IBZProSequenceResultMap" databaseId="mysql">
        <include refid="select" />
		<trim prefix="where" prefixOverrides="and">
			<![CDATA[ AND T1.IBZPRO_SEQUENCEID = #{id} ]]>
		</trim>
    </select>


    <!--通过mybatis将查询结果注入到entity中,通过配置autoMapping="true"由mybatis自动处理映射关系 -->
    <resultMap id="IBZProSequenceResultMap" type="cn.ibizlab.pms.core.ibizpro.domain.IBZProSequence" autoMapping="true">
		<id property="ibzprosequenceid" column="ibzpro_sequenceid" /><!--主键字段映射-->
		<result property="ibzprosequencename" column="ibzpro_sequencename" />
            
		
    </resultMap>


    <!--数据集合[Default]-->
	 <select id="searchDefault"  parameterType="cn.ibizlab.pms.core.ibizpro.filter.IBZProSequenceSearchContext"  resultMap="IBZProSequenceResultMap">
         select t1.* from (
                <include refid="Default" />
         )t1
     </select>

    <!--数据查询[Default]-->
    <sql id="Default" databaseId="mysql">
		<![CDATA[ SELECT t1.`BEGINSEQ`, t1.`CATEGORY`, t1.`CREATEDATE`, t1.`CREATEMAN`, t1.`CURSEQ`, t1.`DEPTID`, t1.`IBZPRO_SEQUENCEID`, t1.`IBZPRO_SEQUENCENAME`, t1.`INCREMENT`, t1.`ISENABLE`, t1.`ORGID`, t1.`UPDATEDATE`, t1.`UPDATEMAN`, t1.`YEAR` FROM `T_IBZPRO_SEQUENCE` t1   ]]>
		<trim prefix="where" prefixOverrides="and">
			<include refid="searchMode" />
		</trim>
    </sql>

    <!--数据查询[Simple]-->
    <sql id="Simple" databaseId="mysql">
		<![CDATA[ SELECT t1.`CREATEDATE`, t1.`CREATEMAN`, t1.`DEPTID`, t1.`IBZPRO_SEQUENCEID`, t1.`IBZPRO_SEQUENCENAME`, t1.`ORGID`, t1.`UPDATEDATE`, t1.`UPDATEMAN` FROM `T_IBZPRO_SEQUENCE` t1   ]]>
		<trim prefix="where" prefixOverrides="and">
			<include refid="searchMode" />
		</trim>
    </sql>

    <!--数据查询[View]-->
    <sql id="View" databaseId="mysql">
		<![CDATA[ SELECT t1.`BEGINSEQ`, t1.`CATEGORY`, t1.`CREATEDATE`, t1.`CREATEMAN`, t1.`CURSEQ`, t1.`DEPTID`, t1.`IBZPRO_SEQUENCEID`, t1.`IBZPRO_SEQUENCENAME`, t1.`INCREMENT`, t1.`ISENABLE`, t1.`ORGID`, t1.`UPDATEDATE`, t1.`UPDATEMAN`, t1.`YEAR` FROM `T_IBZPRO_SEQUENCE` t1   ]]>
		<trim prefix="where" prefixOverrides="and">
			<include refid="searchMode" />
		</trim>
    </sql>

</mapper>

