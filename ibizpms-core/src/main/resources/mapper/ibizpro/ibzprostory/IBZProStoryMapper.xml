<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper	namespace="cn.ibizlab.pms.core.ibizpro.mapper.IBZProStoryMapper">
	
	<!-- 无视图模式 -->
	<sql id="select" databaseId="mysql">
        <![CDATA[ SELECT t1.`ASSIGNEDDATE`, t1.`ASSIGNEDTO`, t1.`BRANCH`, t1.`CHILDSTORIES`, t1.`CLOSEDBY`, t1.`CLOSEDDATE`, t1.`CLOSEDREASON`, t1.`COLOR`, t1.`DELETED`, t1.`DUPLICATESTORY`, t1.`ESTIMATE`, t1.`FROMBUG`, t1.`IBIZ_ID`, t1.`IBIZ_SOURCEID`, t1.`IBIZ_SOURCENAME`, t1.`IBIZ_SOURCEOBJECT`, t1.`ID`, t1.`KEYWORDS`, t1.`LASTEDITEDBY`, t1.`LASTEDITEDDATE`, t1.`LINKSTORIES`, t1.`MAILTO`, t1.`MODULE`, t1.`OPENEDBY`, t1.`OPENEDDATE`, t1.`PRI`, t1.`PRODUCT`, t1.`REVIEWEDBY`, t1.`REVIEWEDDATE`, t1.`SOURCE`, t1.`SOURCENOTE`, t1.`STAGE`, t1.`STAGEDBY`, t1.`STATUS`, t1.`SUBSTATUS`, t1.`TITLE`, t1.`TOBUG`, t1.`TYPE`, t1.`VERSION` FROM `zt_story` t1   ]]>
    </sql>

	
	<!-- 搜索条件 -->
	<sql id="searchMode" databaseId="mysql">
		<if test="srf.n_sourcenote_eq != null ">
			<![CDATA[ AND  t1.`SOURCENOTE` = #{srf.n_sourcenote_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_stagedby_eq != null ">
			<![CDATA[ AND  t1.`STAGEDBY` = #{srf.n_stagedby_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_pri_eq != null ">
			<![CDATA[ AND  t1.`PRI` = #{srf.n_pri_eq,jdbcType=INTEGER} ]]>
        </if>
		<if test="srf.n_stage_eq != null ">
			<![CDATA[ AND  t1.`STAGE` = #{srf.n_stage_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_type_eq != null ">
			<![CDATA[ AND  t1.`TYPE` = #{srf.n_type_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_title_like != null ">
            <bind name="bind_n_title_like" value="'%' + srf.n_title_like + '%'" />
            <![CDATA[ AND  t1.`TITLE` like #{bind_n_title_like,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_ibiz_sourceobject_eq != null ">
			<![CDATA[ AND  t1.`IBIZ_SOURCEOBJECT` = #{srf.n_ibiz_sourceobject_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_ibiz_id_eq != null ">
			<![CDATA[ AND  t1.`IBIZ_ID` = #{srf.n_ibiz_id_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_source_eq != null ">
			<![CDATA[ AND  t1.`SOURCE` = #{srf.n_source_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_color_eq != null ">
			<![CDATA[ AND  t1.`COLOR` = #{srf.n_color_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_status_eq != null ">
			<![CDATA[ AND  t1.`STATUS` = #{srf.n_status_eq,jdbcType=VARCHAR} ]]>
        </if>
		<if test="srf.n_product_eq != null ">
			<![CDATA[ AND  t1.`PRODUCT` = #{srf.n_product_eq,jdbcType=BIGINT} ]]>
        </if>
		<if test="srf.n_module_eq != null ">
			<![CDATA[ AND  t1.`MODULE` = #{srf.n_module_eq,jdbcType=BIGINT} ]]>
        </if>
		<if test="ew!=null and ew.sqlSegment!=null and ew.sqlSegment!=''">
		AND ${ew.sqlSegment}
		</if>
	</sql>


    <!--该方法用于重写mybatis中selectById方法，以实现查询逻辑属性-->
	<select id="selectById"  resultMap="IBZProStoryResultMap" databaseId="mysql">
        <include refid="select" />
		<trim prefix="where" prefixOverrides="and">
			<![CDATA[ AND T1.ID = #{id} ]]>
		</trim>
    </select>


    <!--通过mybatis将查询结果注入到entity中,通过配置autoMapping="true"由mybatis自动处理映射关系 -->
    <resultMap id="IBZProStoryResultMap" type="cn.ibizlab.pms.core.ibizpro.domain.IBZProStory" autoMapping="true">
		<id property="id" column="id" /><!--主键字段映射-->
		<result property="ibizSourcename" column="ibiz_sourcename" />
		<result property="ibizSourceid" column="ibiz_sourceid" />
		<result property="ibizSourceobject" column="ibiz_sourceobject" />
		<result property="ibizid" column="ibiz_id" />
		<result property="product" column="product" />
		<result property="module" column="module" />
            
		
		<!--通过mybatis自动注入关系属性[主实体]，fetchType="lazy"为懒加载配置 -->
		<association property="ibzproproduct" javaType="cn.ibizlab.pms.core.ibizpro.domain.IBZProProduct" column="product" select="cn.ibizlab.pms.core.ibizpro.mapper.IBZProProductMapper.selectById" fetchType="lazy"></association>
		<!--通过mybatis自动注入关系属性[主实体]，fetchType="lazy"为懒加载配置 -->
		<association property="ibzprostorymodule" javaType="cn.ibizlab.pms.core.ibizpro.domain.IBZProStoryModule" column="module" select="cn.ibizlab.pms.core.ibizpro.mapper.IBZProStoryModuleMapper.selectById" fetchType="lazy"></association>
    </resultMap>

	<!--关系实体暴露select方法供主实体通过外键查询关系实体数据[实体关系名称:DER1N_IBZPRO_STORY_IBZPRO_PRODUCT_PRODUCT] -->
	 <select id="selectByProduct" resultMap="IBZProStoryResultMap"  databaseId="mysql">
        <include refid="select" />
		<trim prefix="where" prefixOverrides="and">
			<![CDATA[ AND t1.`PRODUCT`=#{id} ]]>
		</trim>
     </select>
	<!--关系实体暴露select方法供主实体通过外键查询关系实体数据[实体关系名称:DER1N_IBZPRO_STORY_IBZPRO_STORYMODULE_MODULE] -->
	 <select id="selectByModule" resultMap="IBZProStoryResultMap"  databaseId="mysql">
        <include refid="select" />
		<trim prefix="where" prefixOverrides="and">
			<![CDATA[ AND t1.`MODULE`=#{id} ]]>
		</trim>
     </select>

    <!--数据集合[Default]-->
	 <select id="searchDefault"  parameterType="cn.ibizlab.pms.core.ibizpro.filter.IBZProStorySearchContext"  resultMap="IBZProStoryResultMap">
         select t1.* from (
                <include refid="Default" />
         )t1
     </select>

    <!--数据查询[Default]-->
    <sql id="Default" databaseId="mysql">
		<![CDATA[ SELECT t1.`ASSIGNEDDATE`, t1.`ASSIGNEDTO`, t1.`BRANCH`, t1.`CHILDSTORIES`, t1.`CLOSEDBY`, t1.`CLOSEDDATE`, t1.`CLOSEDREASON`, t1.`COLOR`, t1.`DELETED`, t1.`DUPLICATESTORY`, t1.`ESTIMATE`, t1.`FROMBUG`, t1.`IBIZ_ID`, t1.`IBIZ_SOURCEID`, t1.`IBIZ_SOURCENAME`, t1.`IBIZ_SOURCEOBJECT`, t1.`ID`, t1.`KEYWORDS`, t1.`LASTEDITEDBY`, t1.`LASTEDITEDDATE`, t1.`LINKSTORIES`, t1.`MAILTO`, t1.`MODULE`, t1.`OPENEDBY`, t1.`OPENEDDATE`, t1.`PRI`, t1.`PRODUCT`, t1.`REVIEWEDBY`, t1.`REVIEWEDDATE`, t1.`SOURCE`, t1.`SOURCENOTE`, t1.`STAGE`, t1.`STAGEDBY`, t1.`STATUS`, t1.`SUBSTATUS`, t1.`TITLE`, t1.`TOBUG`, t1.`TYPE`, t1.`VERSION` FROM `zt_story` t1   ]]>
		<trim prefix="where" prefixOverrides="and">
			<include refid="searchMode" />
		</trim>
    </sql>
	
	<select id="selectQueryByDefault"  parameterType="cn.ibizlab.pms.core.ibizpro.filter.IBZProStorySearchContext" resultMap="IBZProStoryResultMap">
		<include refid="Default" />
	</select>

    <!--数据查询[View]-->
    <sql id="View" databaseId="mysql">
		<![CDATA[ SELECT t1.`ASSIGNEDDATE`, t1.`ASSIGNEDTO`, t1.`BRANCH`, t1.`CHILDSTORIES`, t1.`CLOSEDBY`, t1.`CLOSEDDATE`, t1.`CLOSEDREASON`, t1.`COLOR`, t1.`DELETED`, t1.`DUPLICATESTORY`, t1.`ESTIMATE`, t1.`FROMBUG`, t1.`IBIZ_ID`, t1.`IBIZ_SOURCEID`, t1.`IBIZ_SOURCENAME`, t1.`IBIZ_SOURCEOBJECT`, t1.`ID`, t1.`KEYWORDS`, t1.`LASTEDITEDBY`, t1.`LASTEDITEDDATE`, t1.`LINKSTORIES`, t1.`MAILTO`, t1.`MODULE`, t1.`OPENEDBY`, t1.`OPENEDDATE`, t1.`PRI`, t1.`PRODUCT`, t1.`REVIEWEDBY`, t1.`REVIEWEDDATE`, t1.`SOURCE`, t1.`SOURCENOTE`, t1.`STAGE`, t1.`STAGEDBY`, t1.`STATUS`, t1.`SUBSTATUS`, t1.`TITLE`, t1.`TOBUG`, t1.`TYPE`, t1.`VERSION` FROM `zt_story` t1   ]]>
		<trim prefix="where" prefixOverrides="and">
			<include refid="searchMode" />
		</trim>
    </sql>
	
	<select id="selectQueryByView"  parameterType="cn.ibizlab.pms.core.ibizpro.filter.IBZProStorySearchContext" resultMap="IBZProStoryResultMap">
		<include refid="View" />
	</select>

</mapper>

