<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper	namespace="cn.ibizlab.pms.core.zentao.mapper.TestResultMapper">
	
	<!-- 无视图模式 -->
	<sql id="select" databaseId="mysql">
        <![CDATA[ SELECT t1.`CASE`, t1.`CASERESULT`, t1.`COMPILE`, t1.`CREATEBY`, t1.`DATE`, t1.`DEPT`, t1.`DEPTNAME`, t1.`DURATION`, t1.`ID`, t1.`JOB`, t1.`LASTRUNNER`, t11.`MODULE`, t21.`NAME` AS `MODULENAME`, t1.`ORG`, t1.`ORGNAME`, t11.`PRECONDITION`, t11.`PRODUCT`, t1.`RUN`, t1.`STEPRESULTS`, t11.`STORY`, t1.`TESTRESULTSN`, t1.`UPDATEBY`, t1.`VERSION`, t1.`XML` FROM `zt_testresult` t1  LEFT JOIN `zt_case` t11 ON t1.`CASE` = t11.`CASESN`  LEFT JOIN `zt_module` t21 ON t11.`MODULE` = t21.`ID`   ]]>
    </sql>

	
	<!-- 搜索条件 -->
	<sql id="searchMode" databaseId="mysql">
		<if test="srf.n_id_eq != null ">
			<![CDATA[ AND  t1.`ID` = #{srf.n_id_eq,jdbcType=BIGINT} ]]>
        </if>
		<if test="srf.n_product_eq != null ">
			<![CDATA[ AND  t11.`PRODUCT` = #{srf.n_product_eq,jdbcType=BIGINT} ]]>
        </if>
		<if test="srf.n_product_like != null ">
            <bind name="bind_n_product_like" value="'%' + srf.n_product_like + '%'" />
            <![CDATA[ AND  t11.`PRODUCT` like #{bind_n_product_like,jdbcType=BIGINT} ]]>
        </if>
		<if test="srf.n_job_eq != null ">
			<![CDATA[ AND  t1.`JOB` = #{srf.n_job_eq,jdbcType=BIGINT} ]]>
        </if>
		<if test="srf.n_case_eq != null ">
			<![CDATA[ AND  t1.`CASE` = #{srf.n_case_eq,jdbcType=BIGINT} ]]>
        </if>
		<if test="srf.n_run_eq != null ">
			<![CDATA[ AND  t1.`RUN` = #{srf.n_run_eq,jdbcType=BIGINT} ]]>
        </if>
		<if test="srf.n_compile_eq != null ">
			<![CDATA[ AND  t1.`COMPILE` = #{srf.n_compile_eq,jdbcType=BIGINT} ]]>
        </if>
		<if test="ew!=null and ew.sqlSegment!=null and ew.sqlSegment!=''">
		AND ${ew.sqlSegment}
		</if>
	</sql>


    <!--该方法用于重写mybatis中selectById方法，以实现查询逻辑属性-->
	<select id="selectById"  resultMap="TestResultResultMap" databaseId="mysql">
        <include refid="select" />
		<trim prefix="where" prefixOverrides="and">
			<![CDATA[ AND T1.ID = #{id} ]]>
		</trim>
    </select>


    <!--通过mybatis将查询结果注入到entity中,通过配置autoMapping="true"由mybatis自动处理映射关系 -->
    <resultMap id="TestResultResultMap" type="cn.ibizlab.pms.core.zentao.domain.TestResult" autoMapping="true">
		<id property="id" column="id" /><!--主键字段映射-->
		<result property="job" column="job" />
		<result property="ibizcase" column="case" />
		<result property="run" column="run" />
		<result property="compile" column="compile" />
            
		
		<!--通过mybatis自动注入关系属性[主实体]，fetchType="lazy"为懒加载配置 -->
		<association property="ztcase" javaType="cn.ibizlab.pms.core.zentao.domain.Case" column="case" select="cn.ibizlab.pms.core.zentao.mapper.CaseMapper.selectById" fetchType="lazy"></association>
		<!--通过mybatis自动注入关系属性[主实体]，fetchType="lazy"为懒加载配置 -->
		<association property="ztcompile" javaType="cn.ibizlab.pms.core.zentao.domain.Compile" column="compile" select="cn.ibizlab.pms.core.zentao.mapper.CompileMapper.selectById" fetchType="lazy"></association>
		<!--通过mybatis自动注入关系属性[主实体]，fetchType="lazy"为懒加载配置 -->
		<association property="ztjob" javaType="cn.ibizlab.pms.core.zentao.domain.Job" column="job" select="cn.ibizlab.pms.core.zentao.mapper.JobMapper.selectById" fetchType="lazy"></association>
		<!--通过mybatis自动注入关系属性[主实体]，fetchType="lazy"为懒加载配置 -->
		<association property="zttestrun" javaType="cn.ibizlab.pms.core.zentao.domain.TestRun" column="run" select="cn.ibizlab.pms.core.zentao.mapper.TestRunMapper.selectById" fetchType="lazy"></association>
    </resultMap>

	<!--关系实体暴露select方法供主实体通过外键查询关系实体数据[实体关系名称:DER1N_ZT_TESTRESULT_ZT_CASE_CASE] -->
	 <select id="selectByIbizcase" resultMap="TestResultResultMap"  databaseId="mysql">
        <include refid="select" />
		<trim prefix="where" prefixOverrides="and">
			<![CDATA[ AND t1.`CASE`=#{id} ]]>
		</trim>
     </select>
	<!--关系实体暴露select方法供主实体通过外键查询关系实体数据[实体关系名称:DER1N_ZT_TESTRESULT_ZT_COMPILE_COMPILE] -->
	 <select id="selectByCompile" resultMap="TestResultResultMap"  databaseId="mysql">
        <include refid="select" />
		<trim prefix="where" prefixOverrides="and">
			<![CDATA[ AND t1.`COMPILE`=#{id} ]]>
		</trim>
     </select>
	<!--关系实体暴露select方法供主实体通过外键查询关系实体数据[实体关系名称:DER1N_ZT_TESTRESULT_ZT_JOB_JOB] -->
	 <select id="selectByJob" resultMap="TestResultResultMap"  databaseId="mysql">
        <include refid="select" />
		<trim prefix="where" prefixOverrides="and">
			<![CDATA[ AND t1.`JOB`=#{id} ]]>
		</trim>
     </select>
	<!--关系实体暴露select方法供主实体通过外键查询关系实体数据[实体关系名称:DER1N_ZT_TESTRESULT_ZT_TESTRUN_RUN] -->
	 <select id="selectByRun" resultMap="TestResultResultMap"  databaseId="mysql">
        <include refid="select" />
		<trim prefix="where" prefixOverrides="and">
			<![CDATA[ AND t1.`RUN`=#{id} ]]>
		</trim>
     </select>

    <!--数据集合[CurTestRun]-->
	 <select id="searchCurTestRun"  parameterType="cn.ibizlab.pms.core.zentao.filter.TestResultSearchContext"  resultMap="TestResultResultMap">
         select t1.* from (
                <include refid="CurTestRun" />
         )t1
     </select>

    <!--数据集合[Default]-->
	 <select id="searchDefault"  parameterType="cn.ibizlab.pms.core.zentao.filter.TestResultSearchContext"  resultMap="TestResultResultMap">
         select t1.* from (
                <include refid="Default" />
         )t1
     </select>

    <!--数据查询[CurTestRun]-->
    <sql id="CurTestRun" databaseId="mysql">
		<![CDATA[ SELECT t1.`CASE`, t1.`CASERESULT`, t1.`COMPILE`, t1.`CREATEBY`, t1.`DATE`, t1.`DEPT`, t1.`DEPTNAME`, t1.`DURATION`, t1.`ID`, t1.`JOB`, t1.`LASTRUNNER`, t11.`MODULE`, t21.`NAME` AS `MODULENAME`, t1.`ORG`, t1.`ORGNAME`, t11.`PRODUCT`, t1.`RUN`, t11.`STORY`, t1.`TESTRESULTSN`, t1.`UPDATEBY`, t1.`VERSION` FROM `zt_testresult` t1  LEFT JOIN `zt_case` t11 ON t1.`CASE` = t11.`CASESN`  LEFT JOIN `zt_module` t21 ON t11.`MODULE` = t21.`ID`  RIGHT JOIN `zt_testrun` t31 ON t1.`RUN` = t31.`TESTRUNSN`   ]]>
		<trim prefix="where" prefixOverrides="and">
			<if test="true">
                <![CDATA[ AND ( t31.`TASK` = #{srf.webcontext.task} ) ]]>
            </if>
			<include refid="searchMode" />
		</trim>
    </sql>
	
	<select id="selectCurTestRun"  parameterType="cn.ibizlab.pms.core.zentao.filter.TestResultSearchContext" resultMap="TestResultResultMap">
		<include refid="CurTestRun" />
	</select>

    <!--数据查询[Default]-->
    <sql id="Default" databaseId="mysql">
		<![CDATA[ SELECT t1.`CASE`, t1.`CASERESULT`, t1.`COMPILE`, t1.`CREATEBY`, t1.`DATE`, t1.`DEPT`, t1.`DEPTNAME`, t1.`DURATION`, t1.`ID`, t1.`JOB`, t1.`LASTRUNNER`, t11.`MODULE`, t21.`NAME` AS `MODULENAME`, t1.`ORG`, t1.`ORGNAME`, t11.`PRODUCT`, t1.`RUN`, t11.`STORY`, t1.`TESTRESULTSN`, t1.`UPDATEBY`, t1.`VERSION` FROM `zt_testresult` t1  LEFT JOIN `zt_case` t11 ON t1.`CASE` = t11.`CASESN`  LEFT JOIN `zt_module` t21 ON t11.`MODULE` = t21.`ID`   ]]>
		<trim prefix="where" prefixOverrides="and">
			<include refid="searchMode" />
		</trim>
    </sql>
	
	<select id="selectDefault"  parameterType="cn.ibizlab.pms.core.zentao.filter.TestResultSearchContext" resultMap="TestResultResultMap">
		<include refid="Default" />
	</select>

    <!--数据查询[View]-->
    <sql id="View" databaseId="mysql">
		<![CDATA[ SELECT t1.`CASE`, t1.`CASERESULT`, t1.`COMPILE`, t1.`CREATEBY`, t1.`DATE`, t1.`DEPT`, t1.`DEPTNAME`, t1.`DURATION`, t1.`ID`, t1.`JOB`, t1.`LASTRUNNER`, t11.`MODULE`, t21.`NAME` AS `MODULENAME`, t1.`ORG`, t1.`ORGNAME`, t11.`PRECONDITION`, t11.`PRODUCT`, t1.`RUN`, t1.`STEPRESULTS`, t11.`STORY`, t1.`TESTRESULTSN`, t1.`UPDATEBY`, t1.`VERSION`, t1.`XML` FROM `zt_testresult` t1  LEFT JOIN `zt_case` t11 ON t1.`CASE` = t11.`CASESN`  LEFT JOIN `zt_module` t21 ON t11.`MODULE` = t21.`ID`   ]]>
		<trim prefix="where" prefixOverrides="and">
			<include refid="searchMode" />
		</trim>
    </sql>
	
	<select id="selectView"  parameterType="cn.ibizlab.pms.core.zentao.filter.TestResultSearchContext" resultMap="TestResultResultMap">
		<include refid="View" />
	</select>

</mapper>

